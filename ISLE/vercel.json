{
    "version": 2,
    "builds": [
      {
        "src": "ISLE/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/isle/browser"
        }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  