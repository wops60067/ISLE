{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/isle/browser"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)\\.(js|css|map|ico|png|jpg|jpeg|svg|woff2?)$", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  